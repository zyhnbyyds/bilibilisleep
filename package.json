{
  "name": "bilibilisleep",
  "type": "commonjs",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_ENV=prod nest build core",
    "dev": "cross-env NODE_ENV=dev nest start bilibilisleep --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "db:gen": "prisma generate",
    "db:deploy": "pnpm prisma migrate deploy",
    "db:dev": "prisma migrate dev",
    "compodoc": "compodoc -p tsconfig.doc.json -d apps-doc -r 3301 -s --language zh-CN",
    "compodoc:init": "compodoc -p tsconfig.doc.json -d apps-doc -r 3301 --language zh-CN",
    "publish": "changeset publish",
    "test": "jest --detectOpenHandles",
    "test:coverage": "jest --coverage --detectOpenHandles"
  },
  "dependencies": {
    "@fastify/compress": "^8.0.3",
    "@fastify/csrf-protection": "^7.1.0",
    "@fastify/one-line-logger": "^2.0.2",
    "@fastify/secure-session": "^8.2.0",
    "@fastify/static": "^8.2.0",
    "@nest-lab/throttler-storage-redis": "^1.1.0",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/bullmq": "^11.0.2",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/common": "^11.1.3",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.3",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/microservices": "^11.1.3",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-fastify": "^11.1.3",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/testing": "^11.1.3",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "^6.10.1",
    "axios": "^1.10.0",
    "bullmq": "^5.56.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cross-env": "^10.0.0",
    "date-fns": "^4.1.0",
    "fastify": "^5.4.0",
    "ioredis": "^5.6.1",
    "lodash": "^4.17.21",
    "luxon": "^3.6.1",
    "ms": "3.0.0-canary.1",
    "nats": "^2.29.3",
    "nest-winston": "^1.10.2",
    "nodemailer": "^7.0.4",
    "openai": "^5.8.2",
    "openapi3-ts": "^4.5.0",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "passport-oauth2": "^1.8.0",
    "prisma": "^6.10.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "swc-loader": "^0.2.6",
    "ulid": "^3.0.1",
    "unplugin-swc": "^1.5.5",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "winston-elasticsearch": "^0.19.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.0.0",
    "@changesets/cli": "^2.29.5",
    "@compodoc/compodoc": "^1.1.26",
    "@nestjs/schematics": "^11.0.5",
    "@swc/core": "^1.12.7",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.19",
    "@types/luxon": "^3.6.2",
    "@types/ms": "^2.1.0",
    "@types/node": "^24.0.7",
    "@types/nodemailer": "^7.0.0",
    "@types/passport-local": "^1.0.38",
    "@types/passport-oauth2": "^1.8.0",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.30.0",
    "jest": "^30.0.3",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "pnpm": {
    "overrides": {
      "sodium-native": "5.0.6"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/shared(|/.*)$": "<rootDir>/libs/shared/src/$1"
    }
  }
}
